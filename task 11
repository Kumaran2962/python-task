a=int(input("enter the week number(1-7): "))
if (a==1):
    print("its Monday")
elif (a==2):
    print("its Tuesday")    
elif (a==3):
    print("its Wednesday")    
elif (a==4):
    print("its Thursday")
elif (a==5):
    print("its Friday")
elif (a==6):
    print("its Saturday")
elif (a==7):
    print("its Sunday")
else:
    print("enter a valid number")